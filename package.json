    "cookie-parser": "^1.4.3",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "csv-parse": "^4.3.4",
    "dotenv": "^7.0.0",
    "easy-soap-request": "^4.1.1",
    "ejs": "^3.1.7",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-fileupload": "^1.2.1",
    "feature-policy": "^0.5.0",
    "helmet": "^3.16.0",
    "http-auth": "^4.1.2",
    "http-auth-connect": "^1.0.4",
    "import-map-overrides": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "0.21.1",
