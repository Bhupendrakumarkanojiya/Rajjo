{
  "name": "tasteup",
  "version": "1.8.88",
  "private": true,
  "description": "taste Server",
  "repository": "git@github.com:carbonfive/taste.git",
  "scripts": {
    "build": "npm run build-ts",
    "build-ts": "tsc",
    "class-central:import-courses": "yarn node ./tasks/class-central importCourses",
    "db:create": "node ./tasks/db create",
    "db:drop": "node ./tasks/db drop",
    "db:migrate": "yarn knex migrate:latest",
    "db:rollback": "yarn knex migrate:rollback",
    "db:seed": "yarn knex seed:run",
    "db:setup": "npm-run-all db:migrate db:seed",
    "db:test:prepare": "NODE_ENV=test npm-run-all db:drop db:create db:migrate db:seed es:reindex",
    "db:test:reset": "NODE_ENV=test npm-run-all db:truncate db:seed es:reindex",
    "db:truncate": "node ./tasks/db truncate",
    "jest": "jest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn run lint --fix",
    "lint:all": "eslint . --ext .js,.jsx,.ts,.tsx 'app/' --color",
    "lint:fix:all": "yarn run lint:all --fix",
    "server": "node -r ts-node/register app/server.ts",
    "start": "ts-node-dev --no-deps --respawn --poll --interval 1000 app/server.ts"
  },
  "dependencies": {
    "@types/cookie-session": "^2.0.41",
    "@types/express-fileupload": "^1.2.0",
    "@types/node": "^14.0.26",
    "@types/pdf-parse": "^1.1.1",
